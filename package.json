{
  "name": "paper-trading-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:clean": "npm run clean:windows && vite build",
    "build:netlify": "npm run clean:windows && vite build",
    "build:windows": "npm run clean:windows && vite build",
    "clean": "rm -rf node_modules/.vite && rm -rf dist && rm -rf .vite",
    "clean:windows": "if exist node_modules\\.vite rmdir /s /q node_modules\\.vite && if exist dist rmdir /s /q dist && if exist .vite rmdir /s /q .vite",
    "clean:linux": "rm -rf node_modules/.vite && rm -rf dist && rm -rf .vite",
    "preview": "vite preview",
    "test": "vitest run",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "deploy:check": "npm run build:clean",
    "deploy:netlify": "netlify deploy --prod",
    "deploy:local": "npm run deploy:check && npm run deploy:netlify"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.38.0",
    "axios": "^1.6.0",
    "clsx": "^2.0.0",
    "date-fns": "^2.30.0",
    "iframe-resizer": "^4.3.9",
    "jstat": "^1.9.6",
    "lucide-react": "^0.263.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.1",
    "recharts": "^2.8.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.3.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "postcss": "^8.4.24",
    "tailwindcss": "^3.3.0",
    "typescript": "^5.0.2",
    "vite": "^5.4.0",
    "vitest": "^0.34.0"
  }
}
